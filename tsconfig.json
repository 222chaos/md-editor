{
  "compilerOptions": {
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "target": "ESNext",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@ant-design/md-to-json-schema": ["src"],
      "@ant-design/md-to-json-schema/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/**/*", "src/MarkDown/demos/.tsx"]
}
